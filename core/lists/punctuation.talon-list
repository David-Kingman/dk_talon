list: user.punctuation
language: en
-

# Custom key commands for using while dictating in a flow of text

# General
full stop: .
dot: .
comma: ,
coma: ,
colon: :
semicolon: ;
quest: ?
slash: /
ampersand: &
exclamation mark: !
underscore: _
dash: " - "
# Quotes
quad: ' "'
end quad: '" '
apostrophe: "'"
# Brackets
smile: (
frown: )
left curly: {
right curly: }
left square: [
right square: ]
# Currencies
dollar sign: $
pound sign: Â£
