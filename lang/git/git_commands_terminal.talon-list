list: user.git_commands_terminal
language: en
-

# File system navigation
list files: ls
change folder: cd
status: git status
remove file: git rm

# Staging
stage: git stage
stage all: git stage .

# Committing
commit: git commit -m

# Restoring
restore: git restore 

# Branching
change branch: git checkout
new branch: git checkout -b
show branches: git branch

# Merging
merge: git merge
merge abort: git merge --abort

# Push and pull
push: git push 
pull: git pull
